{
    "configurations": [
        {
            "name": "AVR",
            "intelliSenseMode": "",
            "compilerPath": "${execPath}/../toolchains/avr-gcc/bin/avr-gcc.exe",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "includePath": [
                "${workspaceFolder}/**",
                "${execPath}/../toolchains/avr-gcc/avr/include",
                "${execPath}/../toolchains/avr-gcc/lib/gcc/avr/12.1.0/include"
            ],
            "compilerArgs": [
                "-g",
                "-Os",
                "-Wall",
                "-Wextra",
                "-fpermissive",
                "-fno-exceptions",
                "-fno-threadsafe-statics",
                "-pipe",
                "-mmcu=atmega328p",
                "-DF_CPU=16000000UL",
                "-Datmega328p"
            ],
            "configurationProvider": "ms-vscode.makefile-tools"
        }
    ],
    "version": 4
}
